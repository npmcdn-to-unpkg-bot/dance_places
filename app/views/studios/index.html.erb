<div class="home-page">
  <div class="wrap">
    <form class="search">
      <input placeholder="Seattle" type="text" id="city" class="search-term"></input>
      <button id="city-btn" class="search-button">
        <i class="fa fa-search" id="search-icon"></i>
      </button>
    </form>
  </div>
  <h1>Dance Studios in Seattle</h1>
  <div class="wrapper">
    <div class="load-bar">
      <div class="load-bar-inner" data-loading="0">
      </div>
    </div>
    <h3>Loading</h3>
    <p>Please wait while we search for the studios...</p>
  </div>
  <ul class="grid">
    <%= render @studios %>
  </ul>
</div>
