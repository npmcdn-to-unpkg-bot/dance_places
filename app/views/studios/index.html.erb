      <button id="city-btn" class="search-button">
        <i class="fa fa-search" id="search-icon"></i>
      </button>
    </form>
  </div>
  <h1>Studios in Seattle</h1>
  <ul class="grid">
    <%= render @studios %>
  </ul>
</div>
